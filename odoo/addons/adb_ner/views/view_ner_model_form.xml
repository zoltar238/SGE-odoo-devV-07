<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_ner_model_form" model="ir.ui.view">
        <field name="name">ner.model.form</field>
        <field name="model">adb_ner.model</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="language"/>
                            <field name="containing_folder"/>
                            <field name="active"/>
                            <field name="created"/>
                        </group>
                        <group>
                            <field name="description"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
