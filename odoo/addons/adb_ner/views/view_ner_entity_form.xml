<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_ner_entity_form" model="ir.ui.view">
        <field name="name">ner.entity.form</field>
        <field name="model">adb_ner.entity</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="entity_type"/>
                        </group>
                        <group>
                            <field name="model_id" widget="many2many_tags"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
